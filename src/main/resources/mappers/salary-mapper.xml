	 <insert id="insertSal" parameterType="Salary" >
	insert into salary
	values (#{salLevel},RPAD(to_number(to_char(SYSDATE, 'yymmdd')),9,SEQ_EMP_ID.NEXTVAL), #{commission},#{meal},#{incomeTax},#{empInsurance},#{retirement},#{healthInsurance},#{total})	
	</insert>

	
	
	
	
