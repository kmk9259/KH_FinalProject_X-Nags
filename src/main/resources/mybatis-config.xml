<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

   <settings><!-- value 에 NULL 은 무조건 대문자로 -->
      <setting name="jdbcTypeForNull" value="NULL" />
   </settings>

   <typeAliases>
   	<typeAlias type="com.kh.spring.member.model.vo.Member" alias="Member" />
    <typeAlias type="com.kh.spring.community.model.vo.Board" alias="Board" /> 
    <typeAlias type="com.kh.spring.mail.model.vo.Mail" alias="Mail"/>
    <typeAlias type="com.kh.spring.employee.model.vo.Employee" alias="Employee"/>
    <typeAlias type="com.kh.spring.employee.model.vo.Job" alias="Job"/>
    <typeAlias type="com.kh.spring.employee.model.vo.Department" alias="Department"/>
    <typeAlias type="com.kh.spring.attendance.model.vo.Attendance" alias="Attendance"/>
    <typeAlias type="com.kh.spring.attendance.model.vo.AttStatus" alias="AttStatus"/>
    <typeAlias type="com.kh.spring.community.model.vo.Board_Reply" alias="Board_Reply" /> 
    <typeAlias type="com.kh.spring.approval.model.vo.Approval" alias="Approval" /> 
    <typeAlias type="com.kh.spring.community.model.vo.Notice" alias="Notice" /> 
	<typeAlias type="com.kh.spring.community.model.vo.Notice_Reply" alias="Notice_Reply" />       
   </typeAliases>


   <mappers>
   	<mapper resource="/mappers/member-mapper.xml" />
    <mapper resource="/mappers/mail-mapper.xml"/> 
    <mapper resource="/mappers/employee-mapper.xml"/>
    <mapper resource="/mappers/board-mapper.xml" />
    <mapper resource="/mappers/attendance-mapper.xml" />
    <mapper resource="/mappers/boardReply-mapper.xml" />
    <mapper resource="/mappers/approval-mapper.xml" />
    <mapper resource="/mappers/notice-mapper.xml" />
	<mapper resource="/mappers/noticeReply-mapper.xml" />
   </mappers>




</configuration>